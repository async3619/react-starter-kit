    "lint-staged": {
        "ignore": [
            "package.json"
        ],
        "linters": {
            "*.{ts,tsx}": [
                "npm run fix-js",
                "git add --force"
            ],
            "*.{json,md,graphql}": [
                "prettier --write",
                "git add --force"
            ],
            "*.{css,less,styl,scss,sass,sss}": [
                "stylelint --fix",
                "git add --force"
            ]
        }
    },
    "scripts": {
        "precommit": "lint-staged",
        "lint-js": "eslint --ext .ts --ext .tsx --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
        "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
        "lint": "yarn run lint-js && yarn run lint-css",
        "fix-js": "yarn run lint-js --fix",
        "fix-css": "yarn run lint-css --fix",
        "fix": "yarn run fix-js && yarn run fix-css",
        "test": "tsc && jest",
        "test-watch": "yarn run test --watch --notify",
        "test-cover": "yarn run test --coverage",
        "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
        "build-stats": "yarn run build --release --analyse",
        "schema": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run schema",
        "clean": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run clean",
        "copy": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run copy",
        "bundle": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run bundle",
        "build": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run build",
        "deploy": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run deploy",
        "render": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run render",
        "serve": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run runServer",
        "start": "env-cmd -f ./tools/envs/.env.dev ts-node tools/run start",
        "codegen": "yarn run schema && graphql-codegen --config codegen.yml",
        "codegen-watch": "graphql-codegen --config codegen.yml --watch"
    }
